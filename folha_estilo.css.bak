* {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
body
{
	margin:0;
	padding:0;
	background:#e5e5e5;	
	color:#333;
	font-family:helvetica;
	font-size:14px;	
}
h2
{
	font-size:20px;	
}
.forms
{
	background-color:#fff;
	border-color:#777;	
	height:25px;
}
.botao
{
	background-color:#FF6347;
	border-color:#FF6347;	
	height:40px;	
	box-shadow: 0 1px 3px 1px rgba(0, 0, 0, 0.3);
	font: bold 14px Roboto;
	color: #fff;	
	min-width:100px;
	text-transform: uppercase;
}
.botao:hover
{
	filter: brightness(120%);
	cursor: pointer;	
}
.botao_limpar
{
	background-color:#888;
	border-color:#888;	
	height:40px;	
	box-shadow: 0 1px 3px 1px rgba(0, 0, 0, 0.3);
	font: bold 14px Roboto;
	color: #fff;
	margin-left:20px;
	min-width:100px;
	text-transform: uppercase;
}
.botao_limpar:hover
{
	filter: brightness(120%);
	cursor: pointer;	
}.quadrado_login
{
	position:absolute;
	top:50%;
	left:50%;
	background:#fff;	
	width:45%;
	transform: translate(-50%, -50%);
	border-radius:3px;
	box-shadow: 0 1px 3px 1px rgba(0, 0, 0, 0.5);
	font-family:Helvetica;
	font-size:16px;
	color:#333;
	padding-top:30px;
	padding-bottom:50px;	
}
.text_login
{
	height:35px;
	width:250px;	
	border-radius:3px;	
	background:#FFE4E1;
	padding-left:10px;
	color:#333;
}
.text_login:focus
{
	border: 1px solid #333;
}
.button_login
{
	height:35px;
	width:250px;	
	border-radius:3px;
	border-color:#FF6347;	
	background:#FF6347;	
	padding-left:10px;
	color:#fff;
	font-size:16px;
	font-stretch:bold;	
}
.button_login:hover
{
	height:35px;
	width:250px;	
	border-radius:3px;	
	background:#FF6347;
	padding-left:10px;
	color:#fff;
	font-size:16px;
	font-stretch:bold;
	opacity : 0.5;
	cursor:pointer;	
}
.conteiner_logo_forms
{
	text-align:center;
	width:100%;	
}
.logo_login
{		
	display:inline-block;
	padding-right:10px;	
}
.formularios_login
{		
	display:inline-block;
	padding-left:10px;
	width:260px;	
}
.senha
{
	font-size: 12px;
	text-decoration:none;
	color:#333;
	text-align:left;
	cursor:pointer;
	width:auto;
}
.senha:hover
{
	text-decoration:underline;	
}
.texto {    
    position: relative;
	color:#333;
    text-align:center;
	width:120px;
	top:70px;
	height:30px;
	overflow:hidden;
	font-size:12px;
	font-weight:bold;	
}
.conteiner
{
	position:relative;
	box-shadow: 0 1px 3px 1px rgba(0, 0, 0, 0.5);
	border-radius: 3px;
	width: 120px;
	height:100px;
	display:inline-block;
	padding-bottom:3px;	
	cursor:pointer;
	margin-left:20px;
	margin-top:20px;	
}
#inicio.conteiner
{
	background:#fff url("imagens/icone_inicio.png") no-repeat top/60px;	
}
#inicio.conteiner:hover
{
	background:#e5e5e5 url("imagens/icone_inicio.png") no-repeat top/65px;	
}
#cadastro.conteiner
{
	background:#fff url("imagens/icone_cadastro.png") no-repeat top/70px;	
}
#cadastro.conteiner:hover
{
	background:#e5e5e5 url("imagens/icone_cadastro.png") no-repeat top/75px;	
}
#consulta.conteiner
{
	background:#fff url("imagens/icone_consulta.png") no-repeat top/60px;	
}
#consulta.conteiner:hover
{
	background:#e5e5e5 url("imagens/icone_consulta.png") no-repeat top/65px;	
}

img.menu {
  width: 80px;  
  display: block;
  margin-left: auto;
  margin-right: auto;
  border:1px solid;
}
img.menu:hover {
  -webkit-transform: scale(1.1);
  -moz-transform: scale(1.1);
  -o-transform: scale(1.1);
  -ms-transform: scale(1.1);
  transform: scale(1.1);
}
#conteiner_menu
{	
	text-align: center;	
}
.mensagem_erro
{
	position: absolute;
	display: none;
	z-index:10;
	box-shadow:0 1px 3px 1px rgba(0, 0, 0, 0.5);
	width: 400px;
	height: 100px;
	text-align: center;
	font-weight: bold;
	background-color: #fff;
	margin-top: 20px;
	font-size: 15px;
	left:50%;
	transform: translate(-50%,0%);
}
.topo_pagina
{
	background:#FF6347;
	height:80px;
	box-shadow:0 1px 3px 1px rgba(0, 0, 0, 0.5);
	padding-left:20px;	
}
.topo_pagina_sig
{
	position:relative;	
	top:50%;
	width:30%;	
	display: inline-block;
	font-size:18px;
	color:#fff;	
	transform: translate(0%, -50%);
	height:0px auto;	
}
.topo_pagina_menu
{
	position:relative;
	top:50%;
	width:30%;	
	display:inline-block;
	font-size:16px;
	font-weight:bold;
	color:#fff;
	float:right;
	transform:translate(0%, -50%);	
	line-height:1.5;
	height:0px auto;
}
a.icone_menu
{
	text-align: center;
	display: block;
	height: 100%;
	width: 100%;
	text-decoration: none;	
}

.rodape
{
	position:relative;
	height: 1000px;
	background-color:#0ff;
}
table.lista
{
	border-collapse:collapse;
	border:0px;	
}
th.vermelho
{
	background-color:#FFF5EE;
	color:#333;
	font-family:Roboto;
	font-size:16px;
	height:30px;
	border:1px solid #777;
	padding: 10px;
}
td.vermelho
{
	background-color:#FFF5EE;
	color:#333;
	font-family:Roboto;
	font-size:16px;
	height:30px;
	border:1px solid #777;
	text-align:center;
}
.tabela_resultados
{
	padding-top:20px;	
}